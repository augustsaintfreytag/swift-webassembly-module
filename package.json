{
	"name": "webassembly-module",
	"version": "0.1.0",
	"description": "Wrapper logic for WebAssembly modules using WASI bindings for easy of integration and inter-process communication.",
	"author": "August Saint Freytag",
	"license": "MIT",
	"private": true,
	"source": "src/index.ts",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"dependencies": {
		"@wasmer/wasi": "^0.10.0",
		"@wasmer/wasmfs": "^0.10.0"
	},
	"devDependencies": {
		"@types/node": "^17.0.10",
		"prettier": "^2.5.1",
		"ttypescript": "^1.5.13",
		"typescript": "^4.5.4",
		"typescript-transform-paths": "^3.3.1"
	},
	"scripts": {
		"build": "yarn run build-clean & yarn run build-sources",
		"build-deferred": "scripts/build-deferred.sh",
		"build-clean": "rm -r dist/* & exit 0",
		"build-sources": "ttsc",
		"postinstall": "stat .development && yarn run build-deferred; true"
	}
}
